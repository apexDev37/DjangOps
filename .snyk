# Snyk (https://snyk.io) policy file, patches or ignores known vulnerabilities
---
version: v1.25.0

language-settings:
  python: "3.12"

ignore: {}
patch: {}

exclude:
  global:
    - .envs/
    - docs/
    - resources/
    - test?/
    - "*.editorconfig"
    - "*.hadolint.yaml"
    - "*.codecov.yml"
    - "*.pre-commit-config.yaml"
    - Makefile
    - README.md
